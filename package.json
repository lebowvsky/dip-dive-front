{
  "name": "dip-dive-front",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:check": "eslint . --max-warnings 0",
    "type-check": "vue-tsc --noEmit",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "lint:style": "stylelint \"src/**/*.{css,scss,vue}\"",
    "lint:style:fix": "stylelint \"src/**/*.{css,scss,vue}\" --fix",
    "format": "npm run prettier:fix && npm run lint:fix && npm run lint:style:fix",
    "format:check": "npm run prettier && npm run lint:check && npm run lint:style && npm run type-check",
    "pre-commit": "npm run format:check",
    "prepare": "husky install || true"
  },
  "dependencies": {
    "oh-vue-icons": "^1.0.0-rc3",
    "vue": "^3.5.13",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@eslint/css": "^0.8.1",
    "@eslint/js": "^9.27.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.27.0",
    "eslint-plugin-vue": "^10.1.0",
    "globals": "^16.2.0",
    "postcss-html": "^1.8.0",
    "prettier": "3.5.3",
    "sass": "^1.89.0",
    "stylelint": "^16.23.1",
    "stylelint-config-recommended-vue": "^1.6.1",
    "stylelint-config-standard-scss": "^15.0.1",
    "stylelint-order": "^7.0.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.32.1",
    "vite": "^6.3.5",
    "vite-plugin-stylelint": "^6.0.2",
    "vue-tsc": "^2.2.8"
  }
}
